<Window x:Class="Popovv.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Popovv"
        mc:Ignorable="d"
        Title="TestWindow" Height="500" Width="500">
    <Grid>
        <Image Name="ImageImg" Source="Resources/med.jpg" RenderOptions.BitmapScalingMode="HighQuality" Stretch="Fill" Width="200" Height="200" Margin="0 0 0 150" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <TextBlock Name="TextTb" Width="400" Height="50" Margin="10 200 10 10"/>
        <Button Name="Button1" Margin="0 400 200 0" Width="150" Height="50" Click="Button1_Click"/>
        <Button Name="Button2" Margin="200 400 0 0" Width="150" Height="50" Click="Button2_Click"/>
    </Grid>
</Window>
